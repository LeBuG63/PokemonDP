            fx:controller="pokdp.PokemonMenu.PokemonMenuController"
            fx:id="pane"
            prefHeight="400.0" prefWidth="600.0">

    <Label fx:id="labelName1"  GridPane.rowIndex="0"/>
    <Label fx:id="labelLevel1" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
    <Label fx:id="labelHP1"    GridPane.rowIndex="0" GridPane.columnIndex="2"/>

    <Label fx:id="labelName2" GridPane.rowIndex="1"/>
    <Label fx:id="labelLevel2" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
    <Label fx:id="labelHP2"    GridPane.rowIndex="1" GridPane.columnIndex="2"/>

    <Label fx:id="labelName3" GridPane.rowIndex="2"/>
