    <Label fx:id="labelHP6"    GridPane.rowIndex="5" GridPane.columnIndex="2"/>


    <fx:define>
        <ArrayList fx:id="labelNameList">
            <fx:reference source="labelName1" />
            <fx:reference source="labelLevel1"/>
            <fx:reference source="labelHP1" />

            <fx:reference source="labelName2" />
            <fx:reference source="labelLevel2" />
            <fx:reference source="labelHP2" />

            <fx:reference source="labelName3" />
            <fx:reference source="labelLevel3" />
            <fx:reference source="labelHP3" />

            <fx:reference source="labelName4" />
            <fx:reference source="labelLevel4" />
            <fx:reference source="labelHP4" />

            <fx:reference source="labelName5" />
            <fx:reference source="labelLevel5" />
            <fx:reference source="labelHP5" />

            <fx:reference source="labelName6" />
            <fx:reference source="labelLevel6" />
            <fx:reference source="labelHP6" />
        </ArrayList>
