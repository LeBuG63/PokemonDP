    <Label fx:id="labelName4" GridPane.rowIndex="3"/>
    <Label fx:id="labelLevel4" GridPane.rowIndex="3" GridPane.columnIndex="1"/>
    <Label fx:id="labelHP4"    GridPane.rowIndex="3" GridPane.columnIndex="2"/>

    <Label fx:id="labelName5" GridPane.rowIndex="4"/>
    <Label fx:id="labelLevel5" GridPane.rowIndex="4" GridPane.columnIndex="1"/>
    <Label fx:id="labelHP5"    GridPane.rowIndex="4" GridPane.columnIndex="2"/>

    <Label fx:id="labelName6" GridPane.rowIndex="5"/>
